{
  "manifest_version": 3,
  "name": "Disable Readonly Inputs",
  "version": "1.0",
  "description": "A Chrome extension to disable readonly inputs on the current page.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}